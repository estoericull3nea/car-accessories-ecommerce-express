<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="css/index.css" />
    {{! <link rel="stylesheet" href="../styles/index.css"> }}

    <!-- boxicons cdn -->
    <link
      rel="stylesheet"
      href="https://unpkg.com/boxicons@latest/css/boxicons.min.css"
    />

    <!-- google fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Nunito:wght@300;400;500;600;700;800;900;1000&display=swap"
      rel="stylesheet"
    />

    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Cabin+Sketch:wght@700&display=swap"
      rel="stylesheet"
    />

    <!-- landing -->

    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.1.3/TweenMax.min.js"
    ></script>

    {{! jquery }}
    <script src="jquery-3.6.4.min.js"></script>
    <script
      src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"
    ></script>

  </head>
  <body class="light">

    <header>

      <div class="logo-container">
        <p class="logo-text">EA</p>
        <a class="logo-item" href="#home"><img
            src="../icons/tire.png"
            alt=""
          /></a>
      </div>

      <ul class="navlist">
        <li><a href="http://localhost:3000/home">Home</a></li>
        <li><a href="#">Products</a></li>
        <li><a href="http://localhost:3000/about_us">About Us</a></li>
        <li><a href="#">Our Team</a></li>
      </ul>

      <div class="btn-container">

        <div class="menu-container">
          <i class="bx bx-menu-alt-right"></i>
        </div>

        <div class="profile-container">
          <i class="bx bx-user"></i>
        </div>

        <div class="search-container">
          <input type="text" placeholder="Search..." />
          <i class="bx bx-search-alt"></i>
        </div>

        <div class="theme-container">
          <span class="default">Light</span>
          <i class="bx bx-chevron-right"></i>

          <ul class="theme-list">
            <li class="item">Light</li>
            <li class="item">Dark</li>
            <li class="item">Game</li>
          </ul>
        </div>

        <div class="cart-container">
          <i class="bx bx-cart"></i>
        </div>

      </div>

    </header>

    <section class="homepage">
      <div class="left-container">
        <h1>Stylish &
          <span>High Performance Car Accessories Essentials</span></h1>
        <p>We're thrilled to have you here, ready to embark on an incredible
          shopping experience.</p>
        <h3>Involve. Inspire. Indulge.</h3>
        <button class="btn shop_now">Shop Now</button>
      </div>

      <div class="right-container">
        <div class="img-con">
          <img src="../images/main.jpg" alt="" />
        </div>
      </div>
    </section>

    <div class="carousel">
      <input type="radio" name="switch" id="id1" />
      <a class="thumb"><img src="../images/main.jpg" /></a>
      <div class="info">
        <a href="#" class="title">House MD</a>
        <span class="ep_num_title">
          <a href="#" class="ep_num">episode 23</a>
          <a href="#" class="ep_title">Moving On</a>
        </span>
        <span class="ep_views_coms">
          <span class="ep_views">214K</span>
          <span class="ep_coms">497</span>
          <span class="time_lapsed">3h ago</span>
        </span>
      </div>

      <input type="radio" name="switch" id="id2" />
      <a class="thumb"><img src="../images/main.jpg" /></a>
      <div class="info">
        <a href="#" class="title">Dexter</a>
        <span class="ep_num_title">
          <a href="#" class="ep_num">episode 11</a>
          <a href="#" class="ep_title">Hello Dexter Morgan</a>
        </span>
        <span class="ep_views_coms">
          <span class="ep_views">196K</span>
          <span class="ep_coms">409</span>
          <span class="time_lapsed">5h ago</span>
        </span>
      </div>

      <input type="radio" name="switch" id="id3" />

      <a class="thumb"><img src="../images/main.jpg" /></a>
      <div class="info">
        <a href="#" class="title">The Killing</a>
        <span class="ep_num_title">
          <a href="#" class="ep_num">episode 13</a>
          <a href="#" class="ep_title">Orpheus Descending</a>
        </span>
        <span class="ep_views_coms">
          <span class="ep_views">172K</span>
          <span class="ep_coms">281</span>
          <span class="time_lapsed">1d ago</span>
        </span>
      </div>
      <input type="radio" name="switch" id="id4" />

      <a class="thumb"><img src="../images/main.jpg" /></a>
      <div class="info">
        <a href="#" class="title">Breaking Bad</a>
        <span class="ep_num_title">
          <a href="#" class="ep_num">episode 16</a>
          <a href="#" class="ep_title">Felina</a>
        </span>
        <span class="ep_views_coms">
          <span class="ep_views">397K</span>
          <span class="ep_coms">616</span>
          <span class="time_lapsed">22h ago</span>
        </span>
      </div>

      <input type="radio" name="switch" id="id5" />

      <a class="thumb"><img src="../images/main.jpg" /></a>
      <div class="info">
        <a href="#" class="title">NCIS</a>
        <span class="ep_num_title">
          <a href="#" class="ep_num">episode 1</a>
          <a href="#" class="ep_title">Truth or Consequences</a>
        </span>
        <span class="ep_views_coms">
          <span class="ep_views">744K</span>
          <span class="ep_coms">859</span>
          <span class="time_lapsed">21m ago</span>
        </span>
      </div>

      <div class="nav_pills">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
        <span></span>
      </div>
    </div>

    <script src="../index.js"></script>

  </body>
</html>